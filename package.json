{
  "name": "slides-writing-bots-for-github",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://denysdovhan.com/slides-writing-bots-for-github",
  "dependencies": {
    "normalize.css": "^8.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "spectacle": "^4.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write '**/*.{js,json}'",
    "deploy": "npm run build && gh-pages -d build -m \"Updated at $(date +'%a, %d %b %Y, %T, %Z')\" --add",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "npm run format",
      "git add"
    ]
  }
}
